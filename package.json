{
	"name": "ws-activity",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint",
		"start": "vue-cli-service serve",
		"build:dev": "vue-cli-service build --mode dev",
		"build:dev1": "vue-cli-service build --mode dev1",
		"build:test": "vue-cli-service build --mode test",
		"build:test1": "vue-cli-service build --mode test1",
		"build:uat": "vue-cli-service build --mode uat",
		"preview": "vue-cli-service build --report",
		"fix": "eslint --fix --ext .js,.vue src --ignore-path .gitignore",
		"svgo": "svgo -f src/icons/svg --config=src/icons/svgo.yml"
	},
	"dependencies": {
		"@amap/amap-jsapi-loader": "^0.0.7",
		"@antv/f2": "^3.8.1",
		"@antv/g2": "^4.1.12",
		"@antv/l7": "^2.3.7",
		"@lottiefiles/lottie-player": "^0.5.1",
		"axios": "^0.21.1",
		"core-js": "^3.6.5",
		"dayjs": "^1.10.4",
		"echarts": "^5.0.2",
		"eruda": "^2.4.1",
		"lottie-web": "^5.7.6",
		"normalize.css": "^8.0.1",
		"vant": "^3.0.6",
		"vue": "^3.0.0",
		"vue-lottie": "^0.2.1",
		"vue-router": "^4.0.0-0",
		"vuex": "^4.0.0-0"
	},
	"devDependencies": {
		"@babel/plugin-proposal-nullish-coalescing-operator": "^7.13.8",
		"@babel/plugin-proposal-optional-chaining": "^7.13.8",
		"@vue/cli-plugin-babel": "~4.5.0",
		"@vue/cli-plugin-eslint": "~4.5.0",
		"@vue/cli-plugin-router": "~4.5.0",
		"@vue/cli-plugin-vuex": "~4.5.0",
		"@vue/cli-service": "~4.5.0",
		"@vue/compiler-sfc": "^3.0.0",
		"@vue/eslint-config-prettier": "^6.0.0",
		"babel-eslint": "^10.1.0",
		"babel-plugin-import": "^1.13.3",
		"babel-plugin-transform-remove-console": "^6.9.4",
		"compression-webpack-plugin": "5.0.1",
		"eslint": "^6.7.2",
		"eslint-plugin-prettier": "^3.1.3",
		"eslint-plugin-vue": "^7.0.0-0",
		"husky": "^5.1.0",
		"less": "^3.0.4",
		"less-loader": "^5.0.0",
		"lint-staged": "^10.5.4",
		"postcss-px-to-viewport": "^1.1.1",
		"prettier": "^1.19.1",
		"script-ext-html-webpack-plugin": "^2.1.5"
	},
	"lint-staged": {
		"src/**/*.{js,json,pcss,vue}": [
			"prettier --write",
			"eslint --fix",
			"git add"
		]
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"keywords": [
		"万顺叫车",
		"拼车",
		"网约车",
		"出租车",
		"共享",
		"合伙人",
		"代驾"
	]
}
